import "strings"
import "types"
import "http"
import "json"
import "tfplan"

req = http.request("https://odexawwp5c.execute-api.us-west-2.amazonaws.com/default/sentinel-remote").with_body(json.marshal(tfplan.resources))

resp = http.post(req)

respData = json.unmarshal(resp.body)


# Main rule
main = rule {
  respData.allow is true
}